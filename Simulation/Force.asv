classdef Force < handle
    %UNTITLED Summary of this class goes here
    %   Detailed explanation goes here
    
    properties
        fv % Force vector [x;y]
        o % Force vector origin [x,y] location
    end
    
    methods
        function obj = Force(fx, fy, x, y)
            obj.fv = [fx;fy];
            obj.o = [x,y];
        end
        function setForceX(obj.fx)
            fv = obj.fv;
            obj.fv = [fx;fv(2,1)];
        end
    end
    
end

